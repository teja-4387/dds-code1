#include<stdio.h>
#include<stdlib.h>
  int main()
{
  int n;
  printf("enter size of array : ");
  scanf("%d",&n);
  int*arr=(int *)malloc(n *sizeof(int));
  for(int i=0;i<n;i++){
printf("\n enter the element of arr[%d]index: ",i);
    scanf("%d",&arr[i]);
  }
  printf("\n entered elements in array: ");
  for(int i=0;i<n;i++){
    printf("%d",arr[i]);
  }
}
    
